<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp">

    <ImageView
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:src="@mipmap/aa44"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:id="@+id/iv_icon" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_toRightOf="@+id/iv_icon">

        <TextView
            android:id="@+id/tv_brand"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#CDCD28"
            android:text="品牌"
            android:textColor="#330612"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"/>

        <TextView
            android:id="@+id/tv_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="大米先生（奥克斯广场店）"
            android:textColor="@color/common_google_signin_btn_text_dark_focused"
            android:textStyle="bold"
            android:layout_toRightOf="@+id/tv_brand"
            android:layout_alignParentTop="true" />

        <TextView
            android:id="@+id/tv_qa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="保 票"
            android:textColor="#666666"
            android:textSize="10sp"
            android:layout_alignParentRight="true"
            android:layout_alignBottom="@+id/tv_name"
            android:layout_alignTop="@+id/tv_name"/>

        <me.zhanghai.android.materialratingbar.MaterialRatingBar
            android:id="@+id/library_wide_ratingbar"
            android:layout_width="50dp"
            android:layout_height="15dp"
            android:layout_marginRight="3dp"
            android:layout_below="@+id/tv_name"
            android:layout_alignParentLeft="true"
            android:rating="4.5"
            app:mrb_progressTint="#E8F536"
            app:mrb_secondaryProgressTint="#E8F536"
            style="@style/Widget.MaterialRatingBar.RatingBar"
            />


        <TextView
            android:id="@+id/tv_evaluation"
            android:layout_width="wrap_content"
            android:layout_height="15dp"
            android:layout_marginBottom="0dp"
            android:text="4.5  月售1403单"
            android:textSize="10sp"
            android:gravity="center"
            android:layout_alignTop="@+id/library_wide_ratingbar"
            android:layout_alignBottom="@+id/library_wide_ratingbar"
            android:layout_toRightOf="@+id/library_wide_ratingbar"/>

        <TextView
            android:id="@+id/tv_punctual"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="2dp"
            android:padding="1dp"
            android:text="准时达"
            android:textColor="#1F56EB"
            android:textSize="10sp"
            android:background="@drawable/punctual_bg"
            android:layout_marginEnd="2dp"
            android:layout_alignTop="@+id/library_wide_ratingbar"
            android:layout_alignBottom="@+id/library_wide_ratingbar"
            android:layout_toLeftOf="@+id/tv_hummingbird"/>

        <TextView
            android:id="@+id/tv_hummingbird"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#378ADD"
            android:padding="1dp"
            android:text="蜂鸟专送"
            android:textColor="@android:color/white"
            android:textSize="10sp"
            android:layout_alignTop="@+id/library_wide_ratingbar"
            android:layout_alignBottom="@+id/library_wide_ratingbar"
            android:layout_alignParentRight="true"/>


        <TextView
            android:id="@+id/tv_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="￥20起送 | 配送费2.5 | ￥15/人"
            android:textSize="10sp"
            android:layout_below="@+id/library_wide_ratingbar"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <TextView
            android:id="@+id/tv_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="1dp"
            android:text="458m | 42分钟"
            android:textSize="8sp"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/tv_price"
            android:layout_alignBottom="@+id/tv_price"
        />

    </RelativeLayout>

